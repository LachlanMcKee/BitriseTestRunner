{
  "bitrise": {
    "appId": "",
    "testApkSourceWorkflow": "",
    "testTriggerWorkflow": ""
  },
  "testData": {
    "hiddenAnnotations" : [
      "org.junit.runner.RunWith"
    ],
    "ignoreTestsWithAnnotations" : [
      "org.junit.Ignore"
    ],
    "allowTestingWithoutFilters": false,
    "flankConfig": {
      "commonYamlFiles": ["flank-template.yaml"],
      "annotationBasedYaml": {
        "options": [
          {
            "annotation": "com.example.SmokeTest",
            "yamlFiles": ["flank-smoke-devices.yaml"]
          },
          {
            "annotation": "com.example.LeakTest",
            "yamlFiles": ["flank-leak-devices.yaml"]
          }
        ],
        "fallbackYamlFiles": ["flank-generic-devices.yaml"]
      }
    },
    "options": [
      {
        "type": "checkbox",
        "label": "Feature1",
        "checkedYamlFiles": ["flank-feature1.yaml"],
        "uncheckedYamlFiles": []
      },
      {
        "type": "drop-down",
        "label": "Presets",
        "values": [
          {
            "label": "",
            "yamlFiles": []
          },
          {
            "label": "Smoke",
            "yamlFiles": ["flank-smoke-devices.yaml", "flank-smoke-preset.yaml"]
          }
        ]
      }
    ]
  }
}
